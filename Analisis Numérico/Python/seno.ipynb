    "import math\n",
    "\n",
    "def seno(x, lim):\n",
    "    sum = x\n",
    "    for i in range(3,lim+1,2):\n",
    "        sum += (math.pow(x,i))/(math.factorial(i))\n",
    "    print(sum)"
   ]
